{
    "$schema": "https://json.schemastore.org/tsconfig",
    "name": "simple-json-data",
    "version": "0.1.0",
    "description": "A simple json data store.",
    "author": "wuzhanggui",
    "license": "MIT",
    "type": "module",
    "keywords": [
        "completing attributes"
    ],
    "homepage": "https://github.com/wurencaideli/simple-json-data",
    "repository": {
        "type": "git",
        "url": "https://github.com/wurencaideli/simple-json-data"
    },
    "bugs": {
        "url": "https://github.com/wurencaideli/simple-json-data/issues"
    },
    "jspm": {
        "main": "dist/simple-json-data.cjs.js",
        "registry": "jspm"
    },
    "main": "dist/simple-json-data.cjs.js",
    "module": "dist/simple-json-data.esm.js",
    "types": "dict/simple-json-data.d.ts",
    "exports": {
        ".": {
            "types": "./dist/simple-json-data.d.ts",
            "import": "./dist/simple-json-data.esm.js",
            "require": "./dist/simple-json-data.cjs.js"
        }
    },
    "files": [
        "dist",
        "src"
    ],
    "scripts": {
        "dev": "rollup -c -w",
        "build": "rollup -c",
        "test": "node demo.js"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^28.0.6",
        "@rollup/plugin-node-resolve": "^16.0.1",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^12.1.4",
        "@types/node": "^24.0.15",
        "rollup": "^3.29.5",
        "tslib": "^2.8.1",
        "typescript": "^5.8.3"
    },
    "dependencies": {
        "fast-sort": "^3.4.1",
        "fuse.js": "^7.1.0",
        "uuid": "^11.1.0"
    }
}